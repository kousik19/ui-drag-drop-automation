                        $stmt = db2_exec($conn, select distinct state from amembms);
                        
                        while($row = db2_fetch_assoc($stmt)) {
                                echo "<option value='". $row["STATE"]. "'>" . $row["STATE"] . "</option>";
                            }
                        
                    ?>
